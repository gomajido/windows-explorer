{
  "name": "fe-vue",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "bunx --bun vite",
    "build": "bunx vue-tsc && bunx --bun vite build",
    "preview": "bunx --bun vite preview --port 8080",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "lint": "eslint src --ext .ts,.vue",
    "lint:fix": "eslint src --ext .ts,.vue --fix",
    "format": "prettier --write src"
  },
  "dependencies": {
    "lucide-vue-next": "^0.300.0",
    "vue": "^3.4.0",
    "vue-router": "^4.6.4",
    "vue-virtual-scroller": "^1.1.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.57.0",
    "@types/node": "^25.0.3",
    "@vitejs/plugin-vue": "^5.0.0",
    "autoprefixer": "^10.4.0",
    "bun-types": "^1.3.5",
    "eslint": "^9.39.2",
    "eslint-plugin-vue": "^10.6.2",
    "postcss": "^8.4.0",
    "prettier": "^3.7.4",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.50.1",
    "vite": "^5.0.0",
    "vue-tsc": "^3.2.1"
  }
}
